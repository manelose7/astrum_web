{
  "version": 2,
  "buildCommand": "pip install -r requirements.txt",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "10mb",
        "includeFiles": [
          "requirements.txt"
        ]
      }
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "keys.txt",
      "use": "@vercel/static"
    },
    {
      "src": "hwid_bindings.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/generate-new-key",
      "dest": "main.py"
    },
    {
      "src": "/list-keys",
      "dest": "main.py"
    },
    {
      "src": "/activate",
      "dest": "main.py"
    },
    {
      "src": "/keys",
      "dest": "main.py"
    },
    {
      "src": "/script.js",
      "dest": "script.js"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "PYTHONPATH": "."
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}
